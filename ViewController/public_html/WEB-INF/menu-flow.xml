<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <view id="ShowCaps">
    <display-name>Capacitaciones</display-name>
    <page>/ShowCaps.jsf</page>
    <redirect/>
  </view>
  <view id="ShowVacaciones">
    <display-name>Vacaciones</display-name>
    <page>/ShowVacaciones.jsf</page>
    <redirect/>
  </view>
  <view id="mainMenu">
    <description>Menu Principal</description>
    <display-name>Menu</display-name>
    <page>/mainMenu.jsf</page>
    <bookmark/>
  </view>
  <control-flow-rule id="__2">
    <from-activity-id>*</from-activity-id>
    <control-flow-case id="__3">
      <from-outcome>adfMenu_ShowCaps</from-outcome>
      <to-activity-id>ShowCaps</to-activity-id>
    </control-flow-case>
    <control-flow-case id="__4">
      <from-outcome>adfMenu_ShowVacaciones</from-outcome>
      <to-activity-id>ShowVacaciones</to-activity-id>
    </control-flow-case>
    <control-flow-case id="__1">
      <from-outcome>adfMenu_mainMenu</from-outcome>
      <to-activity-id>mainMenu</to-activity-id>
    </control-flow-case>
  </control-flow-rule>
  <managed-bean id="__5">
    <description>Menu Model Managed Bean</description>
    <managed-bean-name>root_menu</managed-bean-name>
    <managed-bean-class>oracle.adf.view.rich.model.MDSMenuModel</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
    <managed-property id="__6">
      <property-name>createHiddenNodes</property-name>
      <value>false</value>
    </managed-property>
    <managed-property id="__7">
      <property-name>source</property-name>
      <property-class>java.lang.String</property-class>
      <value>/WEB-INF/root_menu.xml</value>
    </managed-property>
  </managed-bean>
</adfc-config>
